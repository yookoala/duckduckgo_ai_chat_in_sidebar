{
  "manifest_version": 3,
  "name": "DuckDuckGo AI Chat Sidebar",
  "version": "1.2",
  "description": "(Unofficial) Running ChatGPT, Claude, Llama or Mixtral anonymously on a browser sidebar.",
  "browser_specific_settings": {
    "gecko": {
      "id": "{ddf262fb-721f-419f-85c2-4758156cbeb2}"
    }
  },
  "commands": {
    "_execute_sidebar_action": {
      "suggested_key": {
        "mac": "Command+Alt+C",
        "default": "Ctrl+Alt+C"
      },
      "description": "Toggle sidebar action."
    }
  },
  "sidebar_action": {
    "default_title": "DuckDuckGo AI Chat",
    "default_icon": {
      "16": "src/icons/icon.svg",
      "32": "src/icons/icon.svg",
      "48": "src/icons/icon.svg",
      "128": "src/icons/icon.svg"
    },
    "default_panel": "src/sidebar.html"
  },
  "icons": {
    "16": "src/icons/icon.svg",
    "32": "src/icons/icon.svg",
    "48": "src/icons/icon.svg",
    "128": "src/icons/icon.svg"
  }
}
